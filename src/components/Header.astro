---
---

<header class="header">
    <div class="header__container">
        <img src="data:image/svg+xml;base64,"
                 alt="logo Sabadell"/>       
            <button class="button">
                <svg class="svg" xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewBox="0 0 16 16">
                    <path fill-rule="evenodd" d="M2.5 12a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5m0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5m0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5"/>
                  </svg>
            </button>

            <nav class="nav activo">
            <ul class="ul">
               <li class="li"><button class="boton">Acceso</button></li>
                <li class="li"><button class="boton">Hazte Socio</button></li>
            </ul>
        </nav>
        
    </div>
</header>

<script>

const button = document.querySelector('.button')
const nav = document.querySelector('.nav')

if (button !== null && nav !== null) {
    button.addEventListener('click', () => {
        nav.classList.toggle('activo');
    });
}

</script>

<style>
    .header{
        width: 100%;
        height: 72px;
        top: 0;
        left: 0;
        position: fixed;
        background-color: #f5f5f5;
        box-shadow: 0px 2px 8px 0px rgba(0, 0, 0, 0.25);
    }

    .header__container{
        display: flex;
        align-items: center;
        flex-flow: row wrap;
        justify-content: space-between;
        padding: 0px 45px 20px 45px;
    }

    .boton{
        margin: 5px;
        padding: 10px;
        border: 1px solid #006dff;
        font-size: 15px;
        border-radius: 5px;
        background-color: white;
        cursor: pointer;
    }

    .boton:hover{
        background-color: #e1f7ff;
    }

    .header .button{
        display: none;
    }

    .header .svg{
        width: 3.5em;
        height: 3.5em;
        border: none;
    }

    .header .nav{
        display: block;
    }

    .header .ul{
        display: flex;
        flex-flow: row nowrap;
        justify-content: flex-end;
        list-style: none;
    }

    @media screen and (max-width:480px) {

        .header__container{
        padding: 0px;
    }

        .header .nav{
            width: 100%;
            display: none;
            background-color: white;
            margin-top: 15px;
        }

        .header .nav.activo{
            display: block;
        }

        .header .button{
            display: block;
        }

        .header .ul{
            flex-direction: column;
        } 
           
    .boton{
        width: 90%;
    }

    img{
        padding: 25px;
    }       

    nav{
        padding: 25px;
    }

    }
</style>